{
	"name": "Turtlemay's Target Extension",
	"author": "Turtlemay",
	"version": "1.3.1",
	"description": "Extension for Target.com",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": ["https://*.target.com/*"],
			"css": ["css/style.css"],
			"js": ["out/content.js"]
		}
	],
	"web_accessible_resources": [{
		"resources": ["*"],
		"matches": ["*://*/*"]
	}],
	"permissions": ["activeTab", "tabs"],
	"host_permissions": ["https://*.target.com/*"]
}
